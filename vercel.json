{
  "version": 2,
  "builds": [
    {
      "src": "./server/build/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "./client/build",
      "use": "@vercel/static"
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "./server/build/src/index.js"
    }
  ]
}
